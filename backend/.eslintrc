{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": 0,
    "@typescript-eslint/no-unused-vars": 0,
    "@typescript-eslint/consistent-type-definitions": ["error", "type"]
  },
  "env": {
    "es2021": true,
    "node": true
  }
}
